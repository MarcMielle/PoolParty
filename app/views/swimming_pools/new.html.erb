<h1>Add a swimming pool</h1>

<div class="container">
<%= simple_form_for(@swimming_pool) do |f| %>

  <%= f.input :location, as: :string, html5: true %>
  <%= f.input :name, as: :string, html5: true %>
  <%= f.input :description, as: :string, html5: true %>
  <%= f.input :price_per_day %>
  <%= f.input :photo, as: :file %>
  <%= f.submit class: "btn btn-flat-blue" %>
<% end %>

</div>
